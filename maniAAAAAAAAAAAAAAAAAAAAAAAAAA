    Tabs.Misc:AddButton({
        Title = "Chat Translator (Portuguese)",
        Description = "",
        Callback = function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/kehxyzdev/portuguese/main/chat'))()
